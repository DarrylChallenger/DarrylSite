    {
        <link rel="stylesheet" type="text/css" href="~/Content/GreyPprMetal.css" />
    }
</head>
<!--
<body onresize="myFunction()">
-->
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Challenger LLC", "Index", "Home", new { area = "" }, new { @class = "navbar-brand brandtext" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("GreyPprMetal", "GreyPprMetal", "Site")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content" style="height:100%">
        <p class="randomtext" id="rndText"></p>
        @RenderBody()
        <hr />
        <footer class="footer">
            <div class="row">
                <div class="col-xs-12 col-sm-6">
                    <address>
                        <a class="leftFoot" href="@Url.Action("Index", "Home")"><span class="copy">&copy;</span> Darryl Challenger Technology Solutions, Inc.</a>
                    </address>
                </div>
                @*<h2>Contact@dctechnologysolutions.com</p>*@
                <div class="col-xs-12 col-sm-6">
                    <address>
                        @*<a class="rightFoot" href="@Url.Action("Contact", "Home")">contact@dctechnologysolutions.com</a>*@
                        <a class="rightFoot" href="mailto:contact@dctechnologysolutions.com">contact@dctechnologysolutions.com</a>
                    </address>
                </div>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body></html>
